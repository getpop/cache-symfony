{
    "name": "getpop/cache-symfony",
    "type": "library",
    "description": "Implementation of the contracts for caching through Symfony Cache",
    "keywords": [
        "PoP",
        "cache",
        "Symfony"
    ],
    "homepage": "https://github.com/getpop/cache-symfony",
    "license": "MIT",
    "authors": [
        {
            "name": "Leonardo Losoviz",
            "email": "leo@getpop.org",
            "homepage": "https://getpop.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.1",
        "getpop/cache": "dev-master",
        "getpop/root": "^1.0@dev",
        "symfony/cache": "^4.2"
    },
    "require-dev": {
        "phpunit/phpunit" : ">=7.0",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "PoP\\SymfonyCache\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PoP\\SymfonyCache\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
